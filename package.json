{
	"name": "growthopsai",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "vite build",
		"cf-typegen": "wrangler types",
		"deploy": "npm run build && wrangler deploy",
		"dev": "vite",
		"postinstall": "npm run cf-typegen",
		"preview": "vite preview",
		"typecheck": "npm run cf-typegen && vue-tsc --noEmit",
		"start": "vite",
		"migrate": "wrangler d1 migrations apply growthopsai --local",
		"migrate:remote": "wrangler d1 migrations apply growthopsai --remote"
	},
	"dependencies": {
		"@google/genai": "^1.34.0",
		"@tailwindcss/typography": "^0.5.19",
		"clsx": "^2.1.1",
		"hono": "^4.11.3",
		"isbot": "^5.1.31",
		"lucide-vue-next": "^0.562.0",
		"marked": "^12.0.2",
		"pinia": "^2.3.1",
		"tailwind-merge": "^3.4.0",
		"vue": "^3.5.13",
		"vue-router": "^4.5.1",
		"vue-sonner": "^1.3.0"
	},
	"devDependencies": {
		"@cloudflare/vite-plugin": "^1.12.4",
		"@tailwindcss/vite": "^4.1.18",
		"@types/node": "^22",
		"@vitejs/plugin-vue": "^6.0.3",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.9.2",
		"vite": "^7.3.1",
		"vite-plugin-vue-devtools": "^8.0.5",
		"vite-tsconfig-paths": "^6.0.4",
		"vue-tsc": "^3.2.2",
		"wrangler": "^4.60.0"
	}
}
